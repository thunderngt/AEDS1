import requests

# URL para onde a requisição será enviada
url = 'enem.inep.gov.br/participante/api/s/ac/resultado/2023'

# Dados que você deseja enviar no corpo da requisição (pode ser um dicionário, lista, etc.)
dados = {'chave1': 'valor1', 'chave2': 'valor2'}

# Credenciais de login
login = '12887305603'
senha = 'Belinhaa1.'

# Realiza a requisição POST com autenticação básica
response = requests.post(url, data=dados, auth=(login, senha))

# Verifica a resposta
if response.status_code == 200:
    print('Requisição bem-sucedida!')
    print('Resposta:', response.text)
else:
    print('Erro na requisição. Código de status:', response.status_code)
