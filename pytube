import pytube
from pytube import YouTube

# URL do vídeo que você deseja baixar
url = "https://www.youtube.com/watch?v=dQw4w9WgXcQ"

# Cria um objeto YouTube para o vídeo especificado
yt = YouTube(url)

# Seleciona o primeiro stream de vídeo disponível (em geral, o melhor)
video = yt.streams.filter(res='720', file_extension='mp4').first()

# Baixa o vídeo
video.download("C:/Users/Cliente/Videos")

# Seleciona o primeiro stream de vídeo disponível em MP4
